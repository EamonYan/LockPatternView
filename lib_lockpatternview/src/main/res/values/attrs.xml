<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LockPatternView">
        <!--默认的颜色-->
        <attr name="normal_color" format="color"></attr>
        <!--按下的颜色-->
        <attr name="press_color" format="color"></attr>
        <!--通过的颜色-->
        <attr name="pass_color" format="color"></attr>
        <!--错误的颜色-->
        <attr name="error_color" format="color"></attr>
        <!--外圆的半径-->
        <attr name="outer_dot_radius" format="dimension"></attr>
        <!--内圆的半径-->
        <attr name="inner_dot_radius" format="dimension"></attr>
        <!--外圆的圆环的宽度-->
        <attr name="outer_dot_stroke" format="dimension"></attr>
        <!--线的宽度-->
        <attr name="line_stroke" format="dimension"></attr>
    </declare-styleable>
</resources>
